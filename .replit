modules = ["python-3.11", "postgresql-16"]

[nix]
channel = "stable-24_05"
packages = ["cacert", "cairo", "ffmpeg-full", "freetype", "ghostscript", "glibcLocales", "gobject-introspection", "gtk3", "pkg-config", "qhull", "tcl", "tk"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Streamlined App"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Minimal GDPR PDF"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Direct GDPR PDF"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Ultra Simple GDPR PDF"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Simple GDPR PDF"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "GDPR HTML Report"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Reliable GDPR PDF"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Ultra Reliable PDF"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Simple DPIA"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "GDPR Code Scanner"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Clean GDPR Scanner"

[[workflows.workflow]]
name = "Streamlined App"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run streamlined_app.py --server.port 5000 --server.headless true"
waitForPort = 5000

