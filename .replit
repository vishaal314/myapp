modules = ["python-3.11", "postgresql-16", "python3"]

[nix]
channel = "stable-24_05"
packages = ["abseil-cpp", "cacert", "cairo", "cargo", "ffmpeg-full", "fontconfig", "freetype", "ghostscript", "glib", "glibcLocales", "gmp", "gobject-introspection", "gtest", "gtk3", "harfbuzz", "lcms2", "libGL", "libGLU", "libiconv", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "libyaml", "oneDNN", "openjpeg", "openssl", "pango", "pkg-config", "qhull", "re2", "rustc", "tcl", "tesseract", "tk", "unixODBC", "zlib"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Streamlit Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Redis Server"

[[workflows.workflow]]
name = "Streamlit Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app.py --server.port 5000 --server.address 0.0.0.0 --server.headless true"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow]]
name = "Redis Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "redis-server --port 6379 --bind 0.0.0.0"

[workflows.workflow.metadata]
outputType = "console"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3001

[[ports]]
localPort = 6379
externalPort = 4200

[[ports]]
localPort = 8501
externalPort = 3000

[[ports]]
localPort = 33491
externalPort = 5173

[[ports]]
localPort = 37883
externalPort = 5000

[[ports]]
localPort = 38615
externalPort = 3003

[[ports]]
localPort = 42247
externalPort = 3002

[agent]
